{
  "name": "jsx-dom-runtime",
  "version": "0.17.2",
  "description": "A tiny in 500 bytes library to JSX syntax templates for DOM",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "require": "./jsx-runtime/index.cjs",
      "import": "./jsx-runtime/index.js",
      "default": "./jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "require": "./jsx-dev-runtime/index.cjs",
      "import": "./jsx-dev-runtime/index.js",
      "default": "./jsx-dev-runtime/index.js"
    },
    "./babel-preset": {
      "require": "./babel-preset/index.cjs",
      "import": "./babel-preset/index.js",
      "default": "./babel-preset/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}' 'tests/**/*.{js,ts,tsx}'",
    "test": "jest",
    "build": "rollup -c",
    "size": "size-limit",
    "dev": "npm run lint && npm run build && npm test && npm run size"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/helper-plugin-utils": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "csstype": "^3.0.10"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "rollup": "^2.62.0",
    "rollup-plugin-terser": "^7.0.2",
    "simple-git-hooks": "^2.7.0",
    "size-limit": "^7.0.5",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist",
    "jsx-dev-runtime",
    "jsx-runtime",
    "babel-preset",
    "README",
    "LICENSE"
  ],
  "keywords": [
    "jsx",
    "jsxdom",
    "jsx-dom",
    "jsx-runtime"
  ],
  "size-limit": [
    {
      "path": "jsx-runtime/index.js",
      "limit": "495 B"
    },
    {
      "path": "jsx-runtime/index.cjs",
      "limit": "553 B"
    }
  ],
  "simple-git-hooks": {
    "pre-commit": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/jsx-dom-runtime.git"
  },
  "bugs": {
    "url": "https://github.com/shoonia/jsx-dom-runtime/issues"
  },
  "author": {
    "name": "Alexander Zaytsev",
    "email": "zaytsev126@gmail.com",
    "url": "https://twitter.com/_shoonia"
  },
  "homepage": "https://github.com/shoonia/jsx-dom-runtime",
  "license": "MIT"
}
